{
  "description": "Check if a page is getting cached by Varnish and clear the cache of the current page with a click.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/button_gray.png"
  },
  "icons": {
    "16":  "img/icon_small.png",
    "48":  "img/icon_medium.png",
    "128": "img/icon_large.png"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "name": "iMoney Varnish Bot",
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "17",
  "version": "2.0.0"
}
